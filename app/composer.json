{
    "name": "romanroad/rr",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "GPL-3.0-only",
    "type": "project",
    "require": {
        "php": "*",
        "amelia/backblaze": "dev-master",
        "anlutro/l4-settings": "^0.7.0",
        "atayahmet/laravel-nestable": "^0.8.7",
        "barryvdh/laravel-translation-manager": "0.5",
        "christiankuri/laravel-favorite": "^1.2",
        "chumper/zipper": "^1.0",
        "cviebrock/eloquent-sluggable": "^4.5",
        "cybercog/laravel-ban": "^3.1",
        "furqansiddiqui/bip39-mnemonic-php": "*",
        "genealabs/laravel-caffeine": "^0.6.8",
        "grimzy/laravel-mysql-spatial": "^2.0",
        "html2text/html2text": "^4.2",
        "igaster/laravel-theme": "^2.0",
        "illuminate/support": "5.6.*",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "ixudra/curl": "^6.16",
        "jpmurray/laravel-countdown": "^1.3",
        "jralph/twig-markdown": "1.0.1",
        "kris/laravel-form-builder": "^1.14",
        "larapack/config-writer": "1.*",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "leafo/scssphp": "^0.7.7",
        "nahid/talk": "*",
        "nicolaslopezj/searchable": "^1.9",
        "overtrue/laravel-follow": "^1.1",
        "phoenix/eloquent-meta": "^1.9",
        "php-units-of-measure/php-units-of-measure": "^2.1",
        "rcrowe/twigbridge": "^0.9.6",
        "sofa/eloquence-base": "^5.6",
        "sofa/eloquence-metable": "^5.6",
        "spatie/laravel-permission": "*",
        "spatie/laravel-tags": "2.1.5",
        "spiritix/lada-cache": "2.1",
        "torann/laravel-meta-tags": "^3.0",
        "twig/extensions": "^1.5",
        "tymon/jwt-auth": "*",
        "urodoz/truncate-html": "@stable",
        "vinkla/hashids": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "admin",
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Admin\\": "admin/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
